definition
	ACTION[at0000.1] matches {	-- Yoga service
		ism_transition matches {
			ISM_TRANSITION[at0002] matches {	-- Service planned
				current_state matches {
					DV_CODED_TEXT matches {
						defining_code matches {[openehr::526]}
					}
				}
				careflow_step matches {
					DV_CODED_TEXT matches {
						defining_code matches {[local::at0002]}		-- Service planned
					}
				}
			}
			ISM_TRANSITION[at0026] matches {	-- Service request sent
				current_state matches {
					DV_CODED_TEXT matches {
						defining_code matches {[openehr::526]}
					}
				}
				careflow_step matches {
					DV_CODED_TEXT matches {
						defining_code matches {[local::at0026]}		-- Service request sent
					}
				}
			}
			ISM_TRANSITION[at0008] matches {	-- Service postponed
				current_state matches {
					DV_CODED_TEXT matches {
						defining_code matches {[openehr::527]}
					}
				}
				careflow_step matches {
					DV_CODED_TEXT matches {
						defining_code matches {[local::at0008]}		-- Service postponed
					}
				}
			}
			ISM_TRANSITION[at0009] matches {	-- Service cancelled
				current_state matches {
					DV_CODED_TEXT matches {
						defining_code matches {[openehr::528]}
					}
				}
				careflow_step matches {
					DV_CODED_TEXT matches {
						defining_code matches {[local::at0009]}		-- Service cancelled
					}
				}
			}
			ISM_TRANSITION[at0003] matches {	-- Service scheduled
				current_state matches {
					DV_CODED_TEXT matches {
						defining_code matches {[openehr::529]}
					}
				}
				careflow_step matches {
					DV_CODED_TEXT matches {
						defining_code matches {[local::at0003]}		-- Service scheduled
					}
				}
			}
			ISM_TRANSITION[at0.60] matches {	-- Service re-scheduled
				current_state matches {
					DV_CODED_TEXT matches {
						defining_code matches {[openehr::529]}
					}
				}
				careflow_step matches {
					DV_CODED_TEXT matches {
						defining_code matches {[local::at0.60]}		-- Service re-scheduled
					}
				}
			}
			ISM_TRANSITION[at0004.1] matches {	-- Service delivered
				current_state matches {
					DV_CODED_TEXT matches {
						defining_code matches {[openehr::245]}
					}
				}
				careflow_step matches {
					DV_CODED_TEXT matches {
						defining_code matches {[local::at0004.1]}		-- Service delivered
					}
				}
			}
			ISM_TRANSITION[at0010.1] matches {	-- Service suspended
				current_state matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[openehr::
							530, 
							527]
						}
					}
				}
				careflow_step matches {
					DV_CODED_TEXT matches {
						defining_code matches {[local::at0010.1]}		-- Service suspended
					}
				}
			}
			ISM_TRANSITION[at0006] matches {	-- Service abandoned
				current_state matches {
					DV_CODED_TEXT matches {
						defining_code matches {[openehr::531]}
					}
				}
				careflow_step matches {
					DV_CODED_TEXT matches {
						defining_code matches {[local::at0006]}		-- Service abandoned
					}
				}
			}
			ISM_TRANSITION[at0023] matches {	-- Service expired
				current_state matches {
					DV_CODED_TEXT matches {
						defining_code matches {[openehr::531]}
					}
				}
				careflow_step matches {
					DV_CODED_TEXT matches {
						defining_code matches {[local::at0023]}		-- Service expired
					}
				}
			}
			ISM_TRANSITION[at0005.1] matches {	-- Service activity complete
				current_state matches {
					DV_CODED_TEXT matches {
						defining_code matches {[openehr::532]}
					}
				}
				careflow_step matches {
					DV_CODED_TEXT matches {
						defining_code matches {[local::at0005.1]}		-- Service activity complete
					}
				}
			}
		}
		description matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0011.1] occurrences matches {0..1} matches {	-- Service name
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {[local::at0.30]}		-- Yoga
							}
						}
					}
					ELEMENT[at0014.1] occurrences matches {0..1} matches {	-- Asanas done
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0.39] occurrences matches {0..1} matches {	-- Yoga style
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0.40, 	-- Hatha
									at0.41, 	-- Power
									at0.42, 	-- Vinyasa
									at0.43, 	-- Vikram
									at0.44, 	-- Iyengar
									at0.45]	-- Sivananda
								}
							}
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0025.1] occurrences matches {0..1} matches {	-- Scheduled date/time
						value matches {
							DV_DATE_TIME matches {
								value matches {yyyy-mm-ddTHH:MM:SS}
							}
						}
					}
					ELEMENT[at0.31] occurrences matches {0..1} matches {	-- Scheduled duration
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::128]>
								list = <
									["1"] = <
										units = <"min">
										precision = <|0|>
									>
								>
							>
						}
					}
					ELEMENT[at0.48] occurrences matches {0..1} matches {	-- Level
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0.49, 	-- First timer
									at0.50, 	-- Beginner
									at0.51, 	-- Intermediate
									at0.52]	-- Advanced(>5 yrs)
								}
							}
						}
					}
					ELEMENT[at0.53] occurrences matches {0..1} matches {	-- Duration
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::128]>
								list = <
									["1"] = <
										units = <"min">
										precision = <|0|>
									>
								>
							>
						}
					}
					ELEMENT[at0012] occurrences matches {0..1} matches {	-- Reason
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0013] occurrences matches {0..1} matches {	-- Description
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0021] occurrences matches {0..1} matches {	-- Sequence
						value matches {
							DV_COUNT matches {
								magnitude matches {|>=0|}
							}
						}
					}
					allow_archetype CLUSTER[at0027] occurrences matches {0..*} matches {	-- Service detail
						include
							archetype_id/value matches {/.*/}
					}
					allow_archetype CLUSTER[at0029] occurrences matches {0..*} matches {	-- Multimedia
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.multimedia(-[a-zA-Z0-9_]+)*\.v1/}
					}
					ELEMENT[at0028.1] occurrences matches {0..1} matches {	-- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
					allow_archetype CLUSTER[at0.37] occurrences matches {0..*} matches {	-- Feedback
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0015] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0016] occurrences matches {0..1} matches {	-- Requestor identifier
						value matches {
							DV_IDENTIFIER matches {*}
						}
					}
					allow_archetype CLUSTER[at0017] occurrences matches {0..*} matches {	-- Requestor
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0018] occurrences matches {0..1} matches {	-- Receiver identifier
						value matches {
							DV_IDENTIFIER matches {*}
						}
					}
					allow_archetype CLUSTER[at0019] occurrences matches {0..*} matches {	-- Receiver
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0.54] occurrences matches {0..1} matches {	-- Posture correction
						value matches {
							DV_BOOLEAN matches {
								value matches {True, False}
							}
						}
					}
					ELEMENT[at0.55] occurrences matches {0..1} matches {	-- Facility
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0.56] occurrences matches {0..1} matches {	-- Participation mode
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0.33, 	-- Single
									at0.34, 	-- Family
									at0.35]	-- Group
								}
							}
						}
					}
				}
			}
		}
	}

